{
  "id": "disk-warning",
  "title": {
    "en": "Disk health warning"
  },
  "hint": {
    "en": "Triggered when a disk reports health issues or SMART errors."
  },
  "args": [
    {
      "type": "device",
      "name": "device",
      "filter": "driver_id=unraid-server"
    }
  ],
  "tokens": [
    {
      "name": "disk_name",
      "type": "string",
      "title": {
        "en": "Disk Name"
      },
      "example": "disk1"
    },
    {
      "name": "status",
      "type": "string",
      "title": {
        "en": "Status"
      },
      "example": "warning"
    }
  ]
}
