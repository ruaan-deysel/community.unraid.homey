{
  "id": "share-usage-high",
  "title": {
    "en": "Share usage high"
  },
  "hint": {
    "en": "Triggered when a share's usage exceeds 90%."
  },
  "args": [
    {
      "type": "device",
      "name": "device",
      "filter": "driver_id=unraid-server"
    }
  ],
  "tokens": [
    {
      "name": "share_name",
      "type": "string",
      "title": {
        "en": "Share Name"
      },
      "example": "appdata"
    },
    {
      "name": "usage_percent",
      "type": "number",
      "title": {
        "en": "Usage (%)"
      },
      "example": 95
    }
  ]
}
